			s_hist := "AVS CREDITO REF DEPOSITO EM CC " +SA6->A6_NREDUZ    
		ELSE
			s_hist := "DEPOSITOS EFETUADOS" +SA6->A6_NREDUZ    
		END
	Else
		s_hist := "DEP CC "+ alltrim(SA6->A6_NREDUZ)  +"/ " +SE5->E5_CGC
	Endif
