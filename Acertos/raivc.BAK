            DbGoTop()
            DbSetOrder(1)
            If DbSeek(xFilial("SZ9")+SC5->C5_TIPOOP,.T.)
                MTIPOOP:=SZ9->Z9_DESCR
                MQTDEP:= SZ9->Z9_QTDEP
             Endif
       Endif

        Dbselectarea("CANC")
        If MsSeek(mpedido+mItem)
           MCANC:='S'
        Else
            // VALIDA CANCELADOS PARA ITENS CONVERTIDOS
            Dbselectarea("CANC")
             If  MsSeek(mpedido)
               DO While numped==mpedido
                    DO CASE
                          CASE MV_PAR01 $('0101/0102/0103/0104/0105/0114')
