                                   MCODREV=SUBSTR(PRODUTO,1,4)
                    ENDCASE
                    If SUBSTR(PRODUTO,1,4) $(MCODREV)
                      MCANC:='S'
                    Else
                        MCANC:=""
                    Endif
                    DbSkip()
              END
            Else
               MCANC:=""
           Endif
       Endif

        DBSELECTAREA("SF4")
