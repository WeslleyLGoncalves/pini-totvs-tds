        IF DBSEEK(XFILIAL("SF4")+SC6->C6_TES)
              IF SF4->F4_DUPLIC=='N'
                      MPAGOSN:='G'
              ELSE
                      MPAGOSN:='P'
              ENDIF
        ENDIF

        DbSelectArea("SC6")
